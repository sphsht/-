public class Kata
{
  public static int[] squareOrSquareRoot(int[] array)
  {
    for(int i=0; i<array.length; i++){
      var sqrt = Math.sqrt(array[i]);
     if (sqrt==Math.round(sqrt)) {
       array[i]=(int)sqrt;
     }
      else {
        array[i]=array[i]*array[i];
      }
    }
    return array;
  }
}
